<% content_for :rows do %>
  <% @teams.each do |team| %>
    <div class="row">
      <div class="page-header">
        <h2><%= link_to team.alias, team_path(team) %></h2>
      </div>
      <div class="span12">
        <%= render partial: 'services/team', locals: { team: team } %>
      </div>
    </div>
  <% end %>
<% end %>

<%= render partial: 'checks/modal' %>
<%= render partial: 'properties/modal' %>
<%= render partial: 'users/modal' %>
