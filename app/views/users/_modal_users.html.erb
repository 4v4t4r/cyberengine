<table class="table table-hover">
  <thead>
    <tr>
      <th>Service</th>
      <th>Username</th>
      <th>Password</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.service.name %></td>
        <td><%= user.username %></td>
        <td><%= user.password %></td>
      </tr>
    <% end %>
  </tbody>
</table>
