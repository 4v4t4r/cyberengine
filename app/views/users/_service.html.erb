<% service = @service if @service %>
<% users = service.users %>

<table class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>Username</th>
      <th>Password</th>
    </tr>
  </thead>
  <tbody>
    <% if users.count == 0 %>
      <tr>
        <td colspan="2">No Users</td>
      </tr>
    <% else %>
      <% users.each do |user| %>
        <tr>
          <td><%= link_to user.username, team_server_service_user_path(@team,@server,service,user) %></td>
          <td><%= user.password %></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>
